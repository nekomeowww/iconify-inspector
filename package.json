{
  "name": "@nekw/iconify-inspector-root",
  "type": "module",
  "version": "0.0.1",
  "packageManager": "pnpm@10.25.0",
  "description": "Inspector for Iconify icons",
  "author": {
    "name": "Neko Ayaka",
    "email": "neko@ayaka.moe",
    "url": "https://github.com/nekomeowww"
  },
  "license": "MIT",
  "homepage": "https://github.com/nekomeowww/iconify-inspector",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nekomeowww/iconify-inspector.git"
  },
  "bugs": "https://github.com/nekomeowww/iconify-inspector/issues",
  "scripts": {
    "dev": "pnpm -r -F @nekw/iconify-inspector dev",
    "dev:web": "pnpm -rF @nekw/iconify-inspector run dev",
    "dev:apps": "pnpm -rF=\"./apps/*\" run --parallel dev",
    "build": "pnpm -r -F=\"./apps/*\" run build",
    "build:web": "pnpm -rF @nekw/iconify-inspector run build",
    "build:apps": "pnpm -rF=\"./apps/*\" run build",
    "test": "vitest --coverage",
    "test:run": "vitest run",
    "lint": "eslint --cache .",
    "lint:fix": "eslint --cache --fix .",
    "typecheck": "pnpm -rF=\"./packages/*\" -F=\"./apps/*\" -F run --parallel typecheck",
    "up": "taze -w -r -I -f && pnpm prune && pnpm dedupe",
    "changelogithub": "changelogithub",
    "sizecheck": "npx vite-bundle-visualizer",
    "nolyfill": "pnpm dlx nolyfill"
  },
  "devDependencies": {
    "@antfu/eslint-config": "6.7.0",
    "@antfu/ni": "~28.0.0",
    "@antfu/nip": "~0.1.0",
    "@types/node": "~24.10.1",
    "@unocss/eslint-config": "^66.5.10",
    "@unocss/eslint-plugin": "^66.5.10",
    "@unocss/preset-mini": "^66.5.10",
    "bumpp": "^10.3.2",
    "eslint": "^9.39.2",
    "eslint-plugin-import-x": "^4.16.1",
    "rollup": "^4.53.3",
    "taze": "^19.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "unbuild": "^3.6.1",
    "unocss": "66.5.10",
    "unocss-preset-scrollbar": "^3.2.0",
    "vite": "^7.2.7",
    "vite-plugin-inspect": "^11.3.3",
    "vitest": "^4.0.15"
  }
}
